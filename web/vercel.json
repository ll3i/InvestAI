{
  "version": 2,
  "builds": [
    {
      "src": "app_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app_vercel.py"
    }
  ],
  "functions": {
    "app_vercel.py": {
      "runtime": "python3.9"
    }
  }
}